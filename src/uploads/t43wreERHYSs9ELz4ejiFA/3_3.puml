@startuml
package "PPersonnages" {
  class Caract {
    -int Force
    -int Intelligence
    -int Endurance
    -int CaractSecondaire[] //A mettre dans une DB en ligne
  }

  class Personnages {
    -String nom
    -Caract caracteristiques
    -int pv
    -int type // 
    -NomMembre
  }

  class MonstresCommuns {
    -int coordx
    -int coordy
    -String replique
    +xpDonnee(?)
  }

  class MonstresElites {
    -int coordx
    -int coordy
    -String replique
    +xpDonnee(?)
  }

  class Mage {
    -int mana
    -int niveau
  }

  class Guerrier {
    -int niveau
  }

  class XpMonstres {
    -int xpDonnee
  }

  class XpPerso {
    -int xpAct
    -int xp[] //A mettre dans une DB en ligne
    +checkXP(int xpAct, int level)
    +setLevel(int xpAct, int xpMoins, int level)
  }

  ' Relationships
  Caract  --o  Personnages 
  Personnages <|-- MonstresCommuns
  Personnages <|-- MonstresElites
  Personnages <|-- Mage
  Personnages <|-- Guerrier

  MonstresCommuns  o--  XpMonstres
  MonstresElites  o--  XpMonstres
  Mage  o--  XpPerso
  Guerrier  o--  XpPerso
}
@enduml
